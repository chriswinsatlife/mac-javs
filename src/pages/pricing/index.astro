            <div class="athena section-padding large">
              <div class="athena side-padding default">
                <div class="athena max-width-center">
                  <div class="athena max-width medium-s">
                    <div class="athena text-align center">
                      <div class="athena container section-default swiper-container">
                        <h2 class="athena h2">Athena plans and pricing</h2>
                        <div>
                          <div class="athena interactive annual-toggle">
                            <div data-monthlytext class="athena body-default">Monthly</div>
                            <div class="athena custom pricing-annual-container">
                              <div class="embed-autoheight w-embed">
                                <style>
                                  .switch {
                                    position: relative;
                                    display: inline-block;
                                    width: 28.2px;
                                    height: 16px;
                                    margin: 0;
                                  }

                                  .switch input {
                                    opacity: 0;
                                    width: 0;
                                    height: 0;
                                  }

                                  .slider {
                                    position: absolute;
                                    cursor: pointer;
                                    top: 0;
                                    left: 0;
                                    right: 0;
                                    bottom: 0;
                                    background-color: #ccc;
                                    -webkit-transition: 0.4s;
                                    transition: 0.4s;
                                  }

                                  .slider:before {
                                    position: absolute;
                                    content: '';
                                    height: 12.22px;
                                    width: 12.22px;
                                    left: 1.88px;
                                    bottom: 1.88px;
                                    background-color: white;
                                    -webkit-transition: 0.4s;
                                    transition: 0.4s;
                                  }

                                  input:checked + .slider {
                                    background-color: #34c759;
                                  }

                                  input:focus + .slider {
                                    box-shadow: 0 0 1px #34c759;
                                  }

                                  input:checked + .slider:before {
                                    -webkit-transform: translateX(12.22px);
                                    -ms-transform: translateX(12.22px);
                                    transform: translateX(12.22px);
                                  }

                                  .slider.round {
                                    border-radius: 16px;
                                  }

                                  .slider.round:before {
                                    border-radius: 50%;
                                  }
                                </style>
                                <label class="switch">
                                  <input id="plan_toggle" type="checkbox" checked />
                                  <span class="slider round"></span>
                                </label>
                              </div>
                              <div class="athena text-color deepforest"><div class="athena body-default">Annual</div></div>
                              <div data-salepilltop class="athena templated-texts pilled-sale-pale lit">Better savings</div>
                            </div>
                          </div>
                        </div>

                        <div id="w-node-_2446728a-2ee5-244a-ef55-2ec3d774d44b-4dea49a9" class="athena card pricing-base">
