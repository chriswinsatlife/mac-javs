  <script is:inline>
    (function () {
      const nav = document.querySelector('.athena.nav');
      if (nav) {
        const openBtn = nav.querySelector('[data-openbutton]');
        const closeBtn = nav.querySelector('[data-closebutton]');

        function setMobileOpen(isOpen) {
          nav.setAttribute('data-mobile-open', isOpen ? '1' : '0');
        }

        openBtn?.addEventListener('click', () => setMobileOpen(true));
        closeBtn?.addEventListener('click', () => setMobileOpen(false));

        nav.querySelectorAll('a[href]').forEach((a) => {
          a.addEventListener('click', () => {
            const href = a.getAttribute('href') || '';
            if (href.includes('#')) setMobileOpen(false);
          });
        });
      }

      const planToggle = document.getElementById('plan_toggle');
      const monthlyText = document.querySelector('[data-monthlytext]');
      const salePillTop = document.querySelector('[data-salepilltop]');

      const priceEl = document.querySelector('[data-price]');
      const saleValueEl = document.querySelector('[data-salevalue]');
      const frequencyEl = document.querySelector('[data-frequency]');
      const cardToggleEl = document.querySelector('[data-cardtoggle]');
      const monthlyNoticeEl = document.querySelector('[data-monthlynotice]');
      const dynamicCtaEl = document.querySelector('[data-dynamic]');

      let mode = 'annual';
      let payment = 'monthly';

      function setText(el, text) {
        if (!el) return;
        el.textContent = text;
      }

      function setVisible(el, isVisible) {
        if (!el) return;
        el.style.display = isVisible ? '' : 'none';
      }

      function updatePricing() {
        if (!priceEl || !frequencyEl) return;

        if (mode === 'monthly') {
          setVisible(cardToggleEl, false);
          setVisible(saleValueEl, false);
          setVisible(monthlyNoticeEl, true);
          if (monthlyText) monthlyText.style.textDecoration = 'underline';
          if (dynamicCtaEl) dynamicCtaEl.setAttribute('data-loc', 'EA - M Pay - M Commit');

          payment = 'monthly';
          setText(priceEl, '3,600');
          setText(frequencyEl, 'per month');
        } else {
          setVisible(cardToggleEl, true);
          setVisible(monthlyNoticeEl, false);
          setVisible(saleValueEl, true);
          if (monthlyText) monthlyText.style.textDecoration = 'none';

          if (payment === 'monthly') {
            if (dynamicCtaEl) dynamicCtaEl.setAttribute('data-loc', 'EA - M Pay - A Commit');
            setText(priceEl, '3,000');
            setText(frequencyEl, 'per month, annual commitment ($36,000/yr)');
            setText(saleValueEl, 'See upfront pricing to save ~8%');
          } else {
            if (dynamicCtaEl) dynamicCtaEl.setAttribute('data-loc', 'EA - A Pay - A Commit');
            setText(priceEl, '33,000');
            setText(frequencyEl, 'per year, annual commitment');
            setText(saleValueEl, 'Best Value');
          }
        }

        const toggles = cardToggleEl ? Array.from(cardToggleEl.querySelectorAll('.pilled-toggle')) : [];
        toggles.forEach((t) => {
          const tMode = t.getAttribute('data-mode');
          t.classList.toggle('active', tMode === payment);
        });
      }

      function setMode(nextMode) {
        mode = nextMode;
        if (salePillTop) salePillTop.classList.toggle('lit', mode === 'annual');
        updatePricing();
      }

      function togglePayment() {
        payment = payment === 'monthly' ? 'upfront' : 'monthly';
        updatePricing();
      }

      planToggle?.addEventListener('click', () => {
        setMode(planToggle.checked ? 'annual' : 'monthly');
      });

      if (cardToggleEl) {
        cardToggleEl.addEventListener('click', (e) => {
          const target = e.target instanceof Element ? e.target.closest('.pilled-toggle') : null;
          if (!target) return;
          togglePayment();
        });
      }

      setMode(planToggle && planToggle.checked ? 'annual' : 'monthly');
    })();
  </script>
