  <script is:inline>
    (function () {
      const nav = document.querySelector('.athena.nav');
      if (!nav) return;

      const openBtn = nav.querySelector('[data-openbutton]');
      const closeBtn = nav.querySelector('[data-closebutton]');

      function setMobileOpen(isOpen) {
        nav.setAttribute('data-mobile-open', isOpen ? '1' : '0');
      }

      openBtn?.addEventListener('click', () => setMobileOpen(true));
      closeBtn?.addEventListener('click', () => setMobileOpen(false));

      nav.querySelectorAll('a[href]').forEach((a) => {
        a.addEventListener('click', () => {
          const href = a.getAttribute('href') || '';
          if (href.includes('#')) setMobileOpen(false);
        });
      });

      function closeFaq(faq) {
        const content = faq.querySelector('[data-faq-content]');
        const arrow = faq.querySelector('[data-faq-arrow]');
        if (!content) return;
        faq.setAttribute('data-open', '0');
        content.style.height = '0px';
        if (arrow) {
          arrow.style.transform = 'translate3d(0, -50%, 0) scale3d(1, 1, 1) rotateX(0) rotateY(0) rotateZ(0deg) skew(0, 0)';
        }
      }

      function openFaq(faq) {
        const content = faq.querySelector('[data-faq-content]');
        const arrow = faq.querySelector('[data-faq-arrow]');
        if (!content) return;
        faq.setAttribute('data-open', '1');
        content.style.height = content.scrollHeight + 'px';
        if (arrow) {
          arrow.style.transform = 'translate3d(0, -50%, 0) scale3d(1, 1, 1) rotateX(0) rotateY(0) rotateZ(180deg) skew(0, 0)';
        }
      }

      const faqs = Array.from(document.querySelectorAll('[data-faq]'));
      faqs.forEach((faq) => closeFaq(faq));

      document.querySelectorAll('[data-faq-trigger]').forEach((trigger) => {
        trigger.addEventListener('click', () => {
          const faq = trigger.closest('[data-faq]');
          if (!faq) return;
          const isOpen = faq.getAttribute('data-open') === '1';

          faqs.forEach((f) => {
            if (f !== faq) closeFaq(f);
          });

          if (isOpen) closeFaq(faq);
          else openFaq(faq);
        });
      });

      window.addEventListener('resize', () => {
        faqs.forEach((faq) => {
          if (faq.getAttribute('data-open') === '1') {
            const content = faq.querySelector('[data-faq-content]');
            if (!content) return;
            content.style.height = content.scrollHeight + 'px';
          }
        });
      });
    })();
  </script>
